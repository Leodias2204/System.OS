<%- include('partials/header.ejs') %>
<body>
    <div class="container-cadastro">
        <h1>Bem vindo ao OS System</h1>
        <br>
        <h3>Preencha os dados abaixo:</h3>
        <br>
    
        <form class="principal">
        <div class="form-row">
            <div class="col-lg-12">
                <input type="text" placeholder="Condominio"
                    class="form-control my-1 p-4">
            </div>
        </div>    
        <div class="form-row">
            <div class="col-sm-12">
                <input type="text" placeholder="Endereço"
                    class="form-control my-2 p-4">
            </div>
        </div>    
        <div class="form-row">
            <div class="col-sm-12">
                <input type="email" placeholder="Email Condominio"
                    class="form-control my-2 p-4">
            </div>
        </div>    
        <div class="form-row">
            <div class="col-sm-12">
                <input type="tel" placeholder="Telefone"
                    class="form-control my-2 p-4">
            </div>
        </div>    
        <div class="form-row">
            <div class="col-sm-12">
                <input type="number" placeholder="CNPJ"
                    class="form-control my-2 p-4">
            </div>
        </div>    
        <div class="form-row">
            <div class="col-sm-12">
                <input type="text" placeholder="Responsável"
                    class="form-control my-2 p-4">
            </div>
        </div>
        <hr>
        <a href="/"><button type="button" class="btn1">Salvar</button></a>
    </form>

</div>




</body>
<%- include('partials/footer.ejs') %>